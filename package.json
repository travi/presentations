{
  "name": "presentations",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/presentations",
  "bugs": "https://github.com/travi/presentations/issues",
  "homepage": "htpps://presentations.travi.org",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "build": "webpack --env production",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.0.2",
    "@travi/eslint-config-travi": "1.0.0-beta.3",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.13.1",
    "globstar": "1.0.0",
    "husky": "0.12.0",
    "markdownlint-cli": "0.2.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "nyc": "10.0.0",
    "validate-commit-msg": "2.8.2",
    "webpack": "2.2.0-rc.3",
    "webpack-config-utils": "2.3.0"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "spectacle": "1.2.5"
  }
}
